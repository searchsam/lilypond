notes_bass = \relative do {
  si1\p sol1 si2.
  fas1~ fas2. sol2 si2
  re,1 fas1~ fas2 sol2.
  fas1 re1 fas2.
  mi1 fas1 si2.
  si1 sol1 si2.
  fas1~ fas2. sol2 si2
  re,1 fas1~ fas2 sol1
  fas1 re1 fas2.
  mi2 fas1 si2.
  si1 sol1 si2.
  fas1~ fas2. sol2 si2
  fas1~ fas2 sol2.
  fas1 re1 fas2.
  mi2 fas1 si2.
  fas2. si4~ si1
}
