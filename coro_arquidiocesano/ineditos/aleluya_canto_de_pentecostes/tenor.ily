tenor-unfold = \relative do' {
  \compressEmptyMeasures
  \dynamicUp
  \clef "G_8"

  R2.*16 |
  mi4\mf mi8 dos4 dos8 |
  res4 res8 si4 si8 |
  dos4 dos8 dos( si dos) |
  res4. sols,8( sis res) |
  mi4 mi8 dos4 dos8 |
  res4 res8 si4 si8 |
  dos4 dos8 dos( si las) |
  sols2. |

  mi'4\mf mi8 dos4 dos8 |
  res4 res8 si4 si8 |
  dos4 dos8 dos( si dos) |
  res4. sols,8( sis res) |
  mi4 mi8 dos4 dos8 |
  res4 res8 si4 si8 |
  dos4 dos8 dos( si las) |
  sols2. |

  mi'4\mf mi8 dos4 dos8 |
  res4 res8 si4 si8 |
  dos4 dos8 dos( si dos) |
  res4. sols,8( sis res) |
  mi4 mi8 dos4 dos8 |
  res4 res8 si4 si8 |
  dos4 dos8 dos( si las) |
  sols4.~ sols4 r8 |

  R2.*3 |
  r4. r4 sols8\f |
